{
    "name": "Overseerr",
    "id": "overseerr",
    "packaging_format": 1,
    "description": {
        "en": "Request management and media discovery tool for Radarr and Sonarr",
        "fr": "Outil de gestion de demandes et de découverte de médias pour Radarr et Sonarr"
    },
    "version": "1.30.1~ynh1",
    "url": "https://github.com/sct/overseerr",
    "upstream": {
        "license": "MIT",
        "website": "https://overseerr.dev",
        "code": "https://github.com/sct/overseerr"
    },
    "license": "MIT",
    "maintainer": {
        "name": "tituspijean"
    },
    "requirements": {
        "yunohost": ">= 11.0"
    },
    "multi_instance": false,
    "services": [
        "nginx"
    ],
    "arguments": {
        "install" : [
            {
                "name": "domain",
                "type": "domain"
            },
            {
                "name": "path",
                "type": "path"
            },
            {
                "name": "is_public",
                "type": "boolean",
                "default": true
            }
        ]
    }
}
